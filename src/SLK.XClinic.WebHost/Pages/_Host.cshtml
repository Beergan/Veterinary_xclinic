@page "/"
@namespace SLK.XClinic.WebApp.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@using Microsoft.AspNetCore.Authorization
@using Microsoft.AspNetCore.Components.Authorization
@using SLK.XClinic.WebApp

@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="vi">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <title>X-Clinic [v@(SLK.XClinic.WebHost.AutoClass.FullVersion) @SLK.XClinic.WebHost.AutoClass.BuildName]</title>

    <meta name="description" content="iLeaderDrive - Giải pháp quản lý trung tâm đào tạo du học" />
    <meta name="keywords" content="phan mem, quan ly trung tam dao tao du hoc, dien toan dam may" />
    <meta property="og:title" content="iLeaderDrive - Quản lý trung tâm đào tạo du học" />
    <meta property="og:image" content="~/assets/images/logo_ileadernext.png" />
    <meta property="og:description" content="iLeaderDrive.vn - Giải pháp quản lý trung tâm đào tạo du học" />

    <base href="~/" />
    <link rel="icon" href="~/assets/images/favicon-ileadernext.png" />
    <link rel="mask-icon" href="~/assets/images/favicon-ileadernext.png" />
    <link rel="icon" sizes="16x16" href="~/assets/images/favicon-ileadernext.png" />
    <link rel="icon" sizes="192x192" href="~/assets/images/favicon-ileadernext.png" />

    <link rel="stylesheet" href="_content/MudBlazor/MudBlazor.min.css" />

    <link rel="stylesheet" href="assets/bootstrap/4.5.3/css/bootstrap.min.css" />
    <link rel="stylesheet" href="assets/font-awesome/5.15.1/css/all.css" />

    <link rel="stylesheet" href="_content/Syncfusion.Blazor.Themes/bootstrap5.css" />
    <link rel="stylesheet" href="assets/app.css" />
</head>
<body>
    <div id="app">
        <component type="typeof(SLK.XClinic.WebApp.Routes)" render-mode="ServerPrerendered" />
    </div>
    <script src="_framework/blazor.server.js"></script>

    <script src="_content/MudBlazor/MudBlazor.min.js"></script>
    <script src="assets/jquery/jquery-3.5.1.min.js"></script>
    <script src="assets/bootstrap/4.5.3/js/bootstrap.bundle.min.js"></script>
    <script src="_content/Syncfusion.Blazor.Core/scripts/syncfusion-blazor.min.js"></script>
    <script src="_content/Syncfusion.Blazor.WordProcessor/scripts/syncfusion-blazor-documenteditor.min.js"></script>
    <script src="assets/sweetalert2/sweetalert2.js"></script>
    <script src="assets/sweetalert2/sweetalert2-blazor.js"></script>
    <script src="assets/app.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/qrious/4.0.2/qrious.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/html2canvas/1.4.1/html2canvas.min.js"></script>
</body>
</html>
